#include <stdio.h>

int main(void) {
	// your code goes here
	
	char a[]="hello"; 
	int i,v=0,c=0;
	for(i=0;i<5;i++)
	{
		if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'||a[i]=='O'||a[i]=='I'||a[i]=='E'||a[i]=='A'||a[i]=='U')
			v++	;
		else c++;
	}
	printf("vowels =  %d \n",v);
	printf("consonants = %d ",c);

	return 0;
}
